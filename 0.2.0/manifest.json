{
"update_url": "https://clients2.google.com/service/update2/crx",

   "content_scripts": [ {
      "js": [ "jquery-2.1.0.min.js", "PP.js" ],
      "matches": ["http://*/*",
                  "https://*/*"
                 ],
      "run_at": "document_end"
   } ],
